<mapper namespace="user">

    <select id="selectList" resultType="com.lpy.model.User">
        select * from user
    </select>

    <select id="selectOne" parameterType="com.lpy.model.User" resultType="com.lpy.model.User">
        select * from user where id = #{id} and name = #{name}
    </select>

</mapper>